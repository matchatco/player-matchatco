document.addEventListener("DOMContentLoaded",()=>{const t=document.querySelector("video");var e=t.getElementsByTagName("source")[0].src;const l={};if(Hls.isSupported()){const s=new Hls;s.loadSource(e),s.on(Hls.Events.MANIFEST_PARSED,function(e,n){var o=s.levels.map(e=>e.height);l.quality={default:o[0],options:o,forced:!0,onChange:e=>function(o){window.hls.levels.forEach((e,n)=>{e.height===o&&(console.log("Found quality match with "+o),window.hls.currentLevel=n)})}(e)};new Plyr(t,l)}),s.attachMedia(t),window.hls=s}else new Plyr(t,l)});
